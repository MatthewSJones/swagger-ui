


<td class='code {{#if required}} required {{/if}}'>
    <label for='{{valueId}}'>{{name}} ({{{paramType}}})</label>
</td>
<td>
    {{#if isBody}}
        <textarea class='body-textarea' readonly='readonly' {{#if required}} placeholder='(required)' {{/if}}                
                  name='{{name}}' id='{{valueId}}'>{{default}}</textarea>
    {{else}}
        {{#if default}}
            {{default}}
        {{else}}
            (empty)
        {{/if}}
    {{/if}}
</td>
<td class="markdown">{{{description}}}</td>
<td><span class="model-signature"></span></td>


