<td class='code'>
    <label for='{{valueId}}'>{{name}} ({{{paramType}}})</label>
</td>
<td>
    {{#if isBody}}
        {{#if isFile}}
            <input type="file" name='{{name}}' id='{{valueId}}' />
        {{else}}
            <div class="editor_holder"></div>
            <textarea class='body-textarea  
                      {{#if required}} required {{/if}}'                   
                      {{#if required}} placeholder='(required)' {{/if}}
                      name='{{name}}' id="{{valueId}}">
                {{#if default}} {{default}} {{/if}}
            </textarea>
            <br />
            <div class="parameter-content-type" />
        {{/if}}
    {{else}}
        {{#if isFile}}
            <input class='parameter {{#if required}}(required){{/if}}' type='file' name='{{name}}' id='{{valueId}}' />
        {{else}}
            {{#renderTextParam this}}
            {{/renderTextParam}}
        {{/if}}
    {{/if}}
</td>
<td>
    {{#if required}}
        <strong><span class="markdown">{{{description}}}</span></strong>
    {{else}}
        <span class="markdown">{{{description}}}</span>
    {{/if}}
</td>
<td><span class="model-signature"></span></td>

